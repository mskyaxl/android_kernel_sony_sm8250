      "bcl-lvl1",
      "bcl-lvl2";
   #thermal-sensor-cells = <1>;
  };

  pm8150l_adc_tm: adc_tm@3500 {
   compatible = "qcom,adc-tm5";
   reg = <0x3500 0x100>;
   interrupts = <0x4 0x35 0x0 1>;
   interrupt-names = "thr-int-en";
   #address-cells = <1>;
   #size-cells = <0>;
   #thermal-sensor-cells = <1>;
