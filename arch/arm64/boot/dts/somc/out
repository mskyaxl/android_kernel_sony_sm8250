     = <3000000 1500000 1000000 500000>;

   qcom,chg-term-src = <1>;
   qcom,charger-temp-max = <800>;
   qcom,smb-temp-max = <800>;
   status = "disabled";

   qcom,chgr@1000 {
    reg = <0x1000 0x100>;
    interrupts =
     <0x2 0x10 0x0 1>,
     <0x2 0x10 0x1 1>,
