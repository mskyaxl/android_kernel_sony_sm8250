   nvmem = <&pm8150_sdam_2>;
   hr_led1 {
    label = "hr_led1";
    pwms = <&pm8150b_pwm 0 1000000>;
    led-sources = <0>;
   };

   hr_led2 {
    label = "hr_led2";
    pwms = <&pm8150b_pwm 1 1000000>;
    led-sources = <1>;
   };
  };

  pm8150b_haptics: qcom,haptics@c000 {
   compatible = "qcom,haptics";
   reg = <0xc000 0x100>;
   interrupts = <0x3 0xc0 0x0 (2 | 1)>,
